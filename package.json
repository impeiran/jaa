{
  "name": "ure",
  "version": "0.2.0",
  "description": "A lib with some utility functions used frequently",
  "main": "dist/ure.cjs.js",
  "module": "dist/ure.esm.js",
  "unpackaged": "dist/ure.js",
  "types": "dist/ure.d.ts",
  "scripts": {
    "dev": "rollup -w -c rollup.config.ts",
    "build": "rimraf dist && rollup -c rollup.config.ts",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "eslint ./src --ext .js --fix",
    "test": "jest --config jest.config.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/impeiran/ure.git"
  },
  "keywords": [
    "javascript",
    "browser",
    "util",
    "function",
    "natural",
    "ecmascript"
  ],
  "author": "impeiran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/impeiran/ure/issues"
  },
  "homepage": "https://github.com/impeiran/ure#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^25.1.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-dts": "^3.0.1",
    "typescript": "^4.2.3",
    "vuepress": "^1.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.js": "eslint ./src --ext .js"
  }
}
